// loader
.loader
  .page-loader
// Navbar
div
  script(src='js/scripts/navbar.js')
// Floating Shards
img.shard(src='images/demo/shard-1-5x-3.png' alt='Shard')
// Welcome Section
.welcome.d-flex.justify-content-center.flex-column
  .inner-wrapper.mt-auto.mb-auto
    h1.slide-in DDreamkol
    p.slide-in just the database you weren&apos;t expecting
    .action-links.slide-in
      a#scroll-to-content.btn.btn-success.btn-pill.align-self-center(href='#' data-scroll-to='#introduction') Continue
// Page Content
.page-content
  // Content
  .content.clearfix
    #introduction.container.mb-5
      .section-title.col-lg-8.col-md-10.ml-auto.mr-auto(align='text-center')
        p.lead(align='text-center')
          | DDreamkol is a place where you can find pictures and hisotory information about villages of Dolni Drimkol. It&apos;s a database filled for the folk, by the folk and you can contribute too.
    // Cards
    // first 3
    #cards.container.mb-2(style='padding-bottom: 1px')
